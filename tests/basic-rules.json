{
  "creature": {
    "computed": {
      "strengthMod": "floor(strength / 2) - 5",
      "dexterityMod": "floor(dexterity / 2) - 5",
      "constitutionMod": "floor(constitution / 2) - 5",
      "intelligenceMod": "floor(intelligence / 2) - 5",
      "wisdomMod": "floor(wisdom / 2) - 5",
      "charismaMod": "floor(charisma / 2) - 5",

      "baseArmorClass": "10 + dexterityMod",
      "armorClass": "baseArmorClass",

      "strengthSave": "1d20 + strengthMod",
      "dexteritySave": "1d20 + dexterityMod",
      "constitutionSave": "1d20 + constitutionMod",
      "intelligenceSave": "1d20 + intelligenceMod",
      "wisdomSave": "1d20 + wisdomMod",
      "charismaSave": "1d20 + charismaMod"
    },
    "data": {
      "maxHitPoints": {
        "type": "natural"
      },
      "hitPoints": {
        "type": "natural",
        "default": "maxHitPoints"
      },
      "strength": {
        "type": "natural"
      },
      "dexterity": {
        "type": "natural"
      },
      "constitution": {
        "type": "natural"
      },
      "intelligence": {
        "type": "natural"
      },
      "wisdom": {
        "type": "natural"
      },
      "charisma": {
        "type": "natural"
      }
    }
  }
}